{
  "name": "hypertrophy-toolbox",
  "version": "3.0.1",
  "description": "Hypertrophy Toolbox - Workout Planning & Tracking Application",
  "scripts": {
    "build:css": "sass --load-path=node_modules scss/custom-bootstrap.scss static/css/bootstrap.custom.min.css --style compressed",
    "watch:css": "sass --load-path=node_modules scss/custom-bootstrap.scss static/css/bootstrap.custom.min.css --watch",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:headed": "npx playwright test --headed"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "bootstrap": "5.1.3",
    "sass": "^1.69.5",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

